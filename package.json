{
	"private": false,
	"version": "1.0.5",
	"license": "GPL-3.0-or-later",
	"name": "@clevercanyon/utilities.php",
	"description": "Another great project by Clever Canyon.",
	"repository": "https://github.com/clevercanyon/utilities.php",
	"homepage": "https://github.com/clevercanyon/utilities.php#readme",
	"bugs": "https://github.com/clevercanyon/utilities.php/issues",
	"funding": "https://github.com/sponsors/clevercanyon",
	"keywords": [
		"clevercanyon"
	],
	"author": {
		"name": "Clever Canyon",
		"url": "https://clevercanyon.com"
	},
	"contributors": [],
	"type": "module",
	"files": [
		"/dist"
	],
	"bin": {},
	"imports": {},
	"exports": {
		".": {
			"import": "./dist/index.js",
			"require": "./dist/index.umd.cjs",
			"types": "./dist/types/index.d.ts"
		}
	},
	"sideEffects": [
		"./src/*.{html,scss,css,tsx,ts,jsx,mjs,js}"
	],
	"module": "./dist/index.js",
	"main": "./dist/index.umd.cjs",
	"browser": "",
	"unpkg": "./dist/index.umd.cjs",
	"types": "./dist/types/index.d.ts",
	"typesVersions": {
		">=3.1": {
			"./*": [
				"./dist/types/*"
			]
		}
	},
	"dependencies": {},
	"peerDependencies": {},
	"peerDependenciesMeta": {},
	"optionalDependencies": {},
	"bundleDependencies": [],
	"devDependencies": {
		"@clevercanyon/skeleton-dev-deps": "*"
	},
	"overrides": {},
	"cpu": [
		"x64",
		"arm64"
	],
	"os": [
		"darwin",
		"linux"
	],
	"engines": {
		"node": "^19.2.0",
		"npm": "^8.19.3"
	},
	"scripts": {
		"preview": "npx vite preview",
		"dev": "npx vite dev --mode=dev",
		"build:dev": "npx vite build --mode=dev && npx tsc",
		"build:stage": "npx vite build --mode=stage && npx tsc",
		"build:prod": "npx vite build --mode=prod && npx tsc",
		"dotfiles:update": "./dev/.files/bin/update.js"
	},
	"workspaces": [],
	"publishConfig": {
		"registry": "https://npm.pkg.github.com"
	},
	"config": {
		"c10n": {
			"&": {
				"dotfiles": {
					"lock": []
				},
				"build": {
					"appType": "cma",
					"targetEnv": "opl"
				}
			}
		}
	}
}
